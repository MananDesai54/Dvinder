{
  "name": "server",
  "version": "1.0.0",
  "description": "Tinder for Developers With PERN + GraphQL and more",
  "main": "./dist/server.js",
  "repository": "https://github.com/MananDesai54/Dvinder",
  "author": "Manan Desai",
  "license": "MIT",
  "keywords": [
    "typescript",
    "javascript",
    "nodejs",
    "graphql",
    "tinder",
    "apollo",
    "express",
    "ts",
    "js",
    "node.js",
    "MERN",
    "react.js"
  ],
  "scripts": {
    "start": "node dist/server.js",
    "start2": "ts-node src/server.js",
    "dev": "nodemon dist/server.ts",
    "dev2": "nodemon --exec ts-node src/server.ts",
    "watch": "tsc -w",
    "create:migration": "npx mikro-orm migration:create"
  },
  "dependencies": {
    "@mikro-orm/cli": "^4.3.4",
    "@mikro-orm/core": "^4.3.4",
    "@mikro-orm/migrations": "^4.3.4",
    "@mikro-orm/postgresql": "^4.3.4",
    "apollo-server-express": "^2.19.0",
    "argon2": "^0.27.1",
    "connect-redis": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "graphql": "^15.4.0",
    "ioredis": "^4.19.4",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.17",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "uuid": "^8.3.2",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@types/connect-redis": "^0.0.16",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/express-session": "^1.17.3",
    "@types/graphql": "^14.5.0",
    "@types/ioredis": "^4.17.10",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.14",
    "@types/nodemailer": "^6.4.0",
    "@types/uuid": "^8.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
